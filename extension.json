{
	"name": "Ivilution",
	"author": [
		"HM100"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Ivilution",
	"descriptionmsg": "ivilution-desc",
	"version": "1.3.0",
	"license-name": "GPL-3.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.36.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Ivilution\\": "includes/"
	},
	"config": {
		"IvilutionEnableFoo": {
			"description": "Enable the Foo feature.",
			"value": true
		}
	},
	"HookHandlers": {
		"IvilutionHooks": {
			"class": "MediaWiki\\Extension\\Ivilution\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "IvilutionHooks"
	},
	"MessagesDirs": {
		"Ivilution": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Ivilution/resources"
	},
	"ResourceModules": {
		"ext.Ivilution": {
			"targets": [
				"desktop",
				"mobile"
			],
			"dependencies": [],
			"messages": [],
			"styles": ["OOUI.css","Ivilution.css"],
			"scripts": ["chroma.js","init.js","theming.js","base.js"]
		},
		"ext.Ivilution2": {
			"targets": [
				"desktop",
				"mobile"
			],
			"dependencies": [],
			"messages": [],
			"scripts": ["evelution.js"]
		}

	},
	"manifest_version": 2
}
